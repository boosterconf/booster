<article>
<h2 class="page_title">Participant phone list</h2>

<table border="1" cellspacing="0">
  <tr>
    <th>Name</th>
    <th>Role</th>
    <th>Phone number</th>
  </tr>
  <% for user in @users %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.registration.ticket_type_old %></td>
        <td><%= user.phone_number %></td>
      </tr>
  <% end %>
</table>
</article>
